{
    "presets": [
        [
            "@babel/env",
            {
                "targets":
                {
                    "browsers": "> 1%"
                },
                "modules": false,
                "corejs": "3.x",
                "useBuiltIns": "entry",
                "forceAllTransforms": true
            }
        ],
        "@babel/react"
    ],
    "plugins": [
        ["@babel/transform-runtime"]
    ],
    "env":
    {
        "test":
        {
            "presets": [
                "@babel/env",
                "@babel/react"
            ],
            // "plugins": [
            //     "react-hot-loader/babel",
            // ]
        }
    }
}